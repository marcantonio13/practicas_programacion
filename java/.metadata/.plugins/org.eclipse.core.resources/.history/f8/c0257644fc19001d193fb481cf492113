package primer_regla;

public class ClaseUno {

	public static void main(String[] args) {
		int variable1 = 5;
		int variable2 = 10;  
		if(variable1 > variable2) {
			System.out.println("varible1 es mayor que varible2");
		} else {
			System.out.println("varible2 es mayor que varible1");
			
		}
		
	}

}
